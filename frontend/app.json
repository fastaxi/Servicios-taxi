{
  "expo": {
    "name": "TaxiFast",
    "slug": "taxifast-saas",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "taxifast",
    "userInterfaceStyle": "automatic",
    "description": "TaxiFast - Plataforma SaaS Multi-tenant para gesti√≥n de flotas de taxi",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0066CC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taxifast.saas",
      "buildNumber": "2.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0066CC"
      },
      "package": "com.taxifast.saas",
      "versionCode": 4,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0066CC"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7492bcf8-48ca-41ba-96e6-920db5fb56eb"
      },
      "EXPO_PUBLIC_BACKEND_URL": "https://taxitineo.emergent.host"
    }
  }
}
